<keymap version="1" name="Universal" parent="$default">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl c"/>
    <keyboard-shortcut first-keystroke="meta c"/>
  </action>
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="ctrl x"/>
    <keyboard-shortcut first-keystroke="meta x"/>
    <keyboard-shortcut first-keystroke="shift delete"/>
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="ctrl v"/>
    <keyboard-shortcut first-keystroke="meta v"/>
  </action>
  <action id="$SelectAll">
    <keyboard-shortcut first-keystroke="ctrl a"/>
    <keyboard-shortcut first-keystroke="meta a"/>
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="alt left"/>
    <mouse-shortcut keystroke="button4"/>
    <keyboard-shortcut first-keystroke="meta open_bracket"/>
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="alt right"/>
    <mouse-shortcut keystroke="button5"/>
    <keyboard-shortcut first-keystroke="meta close_bracket"/>
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl w"/>
    <keyboard-shortcut first-keystroke="meta w"/>
  </action>
  <action id="EditorToggleCase">
    <keyboard-shortcut first-keystroke="ctrl up"/>
    <keyboard-shortcut first-keystroke="ctrl down"/>
  </action>
  <action id="Replace">
    <keyboard-shortcut first-keystroke="ctrl r"/>
    <keyboard-shortcut first-keystroke="meta r"/>
  </action>
  <action id="ReplaceInPath">
    <keyboard-shortcut first-keystroke="shift ctrl r"/>
    <keyboard-shortcut first-keystroke="shift meta r"/>
  </action>
  <action id="Find">
    <keyboard-shortcut first-keystroke="ctrl f"/>
    <keyboard-shortcut first-keystroke="meta f"/>
  </action>
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="shift ctrl f"/>
    <keyboard-shortcut first-keystroke="shift meta f"/>
  </action>
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="alt o"/>
    <keyboard-shortcut first-keystroke="ctrl o"/>
    <keyboard-shortcut first-keystroke="meta o"/>
  </action>
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift alt o"/>
    <keyboard-shortcut first-keystroke="shift ctrl o"/>
    <keyboard-shortcut first-keystroke="shift meta o"/>
  </action>
  <action id="GotoDeclaration">
    <mouse-shortcut keystroke="alt button1"/>
    <mouse-shortcut keystroke="ctrl button1"/>
  </action>
  <action id="osmedile.intellij.stringmanip.ToLowerCaseAction">
    <keyboard-shortcut first-keystroke="ctrl down"/>
  </action>
  <action id="osmedile.intellij.stringmanip.ToUpperCaseAction">
    <keyboard-shortcut first-keystroke="ctrl up"/>
  </action>
</keymap>